{
    "name": "yurunsoft/nacos-php",
    "description": "Nacos PHP client, which also supports Swoole Coroutine",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.4",
        "yurunsoft/yurun-http": "^4.3|^5.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9.5",
        "swoole/ide-helper": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Yurun\\Nacos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yurun\\Nacos\\Test\\": "tests/"
        }
    },
    "suggest": {
        "monolog/monolog": "Support log"
    },
    "authors": [
        {
            "name": "Yurun",
            "email": "admin@yurunsoft.com"
        }
    ],
    "scripts": {
        "test": "@php vendor/bin/phpunit -c ./phpunit.xml"
    }
}
